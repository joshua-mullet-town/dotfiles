{
  "global": {
    "playwright": {
      "type": "stdio",
      "command": "npx",
      "args": [
        "@playwright/mcp@latest"
      ],
      "env": {}
    },
    "command-hub": {
      "type": "stdio",
      "command": "${CODE_DIR}/hub/start-mcp-server.sh",
      "args": [],
      "env": {}
    },
    "word-display-poc": {
      "type": "stdio",
      "command": "${CODE_DIR}/chatgpt-app-poc/backend/start-mcp-server.sh",
      "args": [],
      "env": {}
    },
    "dynamodb": {
      "type": "stdio",
      "command": "${HOME}/Library/Python/3.13/bin/uvx",
      "args": [
        "awslabs.dynamodb-mcp-server@latest"
      ],
      "env": {
        "AWS_REGION": "us-east-1"
      }
    },
    "aws-unified": {
      "type": "stdio",
      "command": "${HOME}/Library/Python/3.13/bin/uvx",
      "args": [
        "awslabs.core-mcp-server@latest"
      ],
      "env": {
        "AWS_REGION": "us-east-1"
      }
    },
    "bronco-browser": {
      "command": "npx",
      "args": [
        "-y",
        "bronco-browser",
        "serve"
      ]
    },
    "jira-mcp-server": {
      "type": "stdio",
      "command": "uvx",
      "args": [
        "--from",
        "git+ssh://git@github-tr/Tire-Rack-Innovation/jira-mcp-server",
        "jira-mcp-server"
      ],
      "env": {
        "JIRA_URL": "https://asaptire.atlassian.net",
        "JIRA_USERNAME": "${JIRA_USERNAME}",
        "JIRA_API_TOKEN": "${JIRA_API_TOKEN}",
        "JIRA_WRITE_ENABLED": "true",
        "JIRA_ALLOWED_PROJECTS": "TRAC,FRON"
      }
    },
    "langfuse-docs": {
